{
  "name": "my-project",
  "private": true,
  "scripts": {
    "start:frontend": "cd frontend && ng serve",
    "start:backend": "cd backend/fin-backend && dotnet run",
    "build:frontend": "cd frontend && ng build --prod",
    "build:backend": "cd backend && dotnet build",
    "install:frontend": "cd frontend && npm install",
    "restore:backend": "cd backend && dotnet restore",
    "dev": "concurrently \"npm run start:backend\" \"npm run start:frontend\""
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  }
}
